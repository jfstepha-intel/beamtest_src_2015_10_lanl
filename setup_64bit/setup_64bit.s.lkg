main64:
 xor %rax, %rax
 mov $0xdeadbeeffeed1234, %rax
 lea (msg_64bit_started), %ecx
 or $0xf0000000, %ecx    # because i manually relocate code

  print_string_ser_poll:
    mov $0x3fd, %edx
    in %dx, %al
    test $0x20, %al
    jz print_string_ser_poll
    mov (%ecx), %al
    mov $0x3f8, %edx
    out %al, %dx
    inc %ecx
    mov (%ecx), %al
    test %al, %al
    jnz print_string_ser_poll
